   "source": [
    "# Write a Python program to calculate the sum of all even numbers between 1 and a given positive integer n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d7fda9b4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a positive integer: 19\n",
      "The sum of all even numbers between 1 and 19 is 90\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a positive integer: \"))\n",
    "sum_even = sum(range(2, n + 1, 2))\n",
    "print(f\"The sum of all even numbers between 1 and {n} is {sum_even}\")"
   ]
